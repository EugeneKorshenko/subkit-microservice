<!DOCTYPE html>
<html>
<head>
  <title>SubKit</title>
  <link href='//fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css'/>
  <link href='public/css/highlight.css' media='screen' rel='stylesheet' type='text/css'/>
  <link href='public/css/screen.css' media='screen' rel='stylesheet' type='text/css'/>
  <script src="public/lib/doc/shred.bundle.js" type="text/javascript"></script>  
  <script src='public/lib/doc/jquery-1.8.0.min.js' type='text/javascript'></script>
  <script src='public/lib/doc/jquery.slideto.min.js' type='text/javascript'></script>
  <script src='public/lib/doc/jquery.wiggle.min.js' type='text/javascript'></script>
  <script src='public/lib/doc/jquery.ba-bbq.min.js' type='text/javascript'></script>
  <script src='public/lib/doc/handlebars-1.0.0.js' type='text/javascript'></script>
  <script src='public/lib/doc/underscore-min.js' type='text/javascript'></script>
  <script src='public/lib/doc/backbone-min.js' type='text/javascript'></script>
  <script src='public/lib/doc/swagger.js' type='text/javascript'></script>
  <script src='public/lib/doc/swagger-ui.js' type='text/javascript'></script>
  <script src='public/lib/doc/highlight.7.3.pack.js' type='text/javascript'></script>
  <script type="text/javascript">
    $(function () {
      window.swaggerUi = new SwaggerUi({
      url: "http://",
      dom_id: "swagger-ui-container",
      supportedSubmitMethods: ['get', 'post', 'put', 'delete'],
      onComplete: function(swaggerApi, swaggerUi){
        if(console) {
          console.log("Loaded API Doc UI")
        }
        $('pre code').each(function(i, e) {hljs.highlightBlock(e)});
      },
      onFailure: function(data) {
        if(console) {
          console.log("Unable to Load API Doc UI");
          console.log(data);
        }
      },
      docExpansion: "none"
    });

    $('#input_apiKey').change(function() {
      var key = $('#input_apiKey')[0].value;
      console.log("key: " + key);
      if(key && key.trim() != "") {
        console.log("added key " + key);
            window.authorizations.add("key", new ApiKeyAuthorization("api_key", key, "header"));
      }
    })
    $('#input_username, #input_password').change(function() {
      var username = $('#input_username')[0].value;
      var password = $('#input_password')[0].value;
      console.log("username: " + username);
      console.log("password: " + password);
      if(username && username.trim() != "" && password && password.trim() != "" ) {
        window.authorizations.add("basic", new BasicAuthorization(username, password));
      }
    })

    // window.swaggerUi.load();
  });

  </script>
</head>

<body>
<div id='header'>
  <div class="swagger-ui-wrap">
    <a id="logo" href="http://subkit.io">SubKit</a>
    <form id='api_selector'>
      <div class='input'><input placeholder="" id="input_baseUrl" name="baseUrl" type="text"/></div>
      <div class='input'><input placeholder="api_key" id="input_apiKey" name="apiKey" type="text"/></div>
      <div class='input'><input placeholder="username" id="input_username" name="username" type="text"/></div>
      <div class='input'><input placeholder="password" id="input_password" name="password" type="text"/></div>
      <div class='input'><a id="explore" href="#">Explore</a></div>
    </form>
  </div>
</div>

<div id="message-bar" class="swagger-ui-wrap">
  &nbsp;
</div>

<div id="swagger-ui-container" class="swagger-ui-wrap">
</div>

</body>
</html>